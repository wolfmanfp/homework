<?xml version="1.0" ?>
<robot name="roscar" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find roscar_description)/urdf/roscar.gazebo" />

  <link name="chassis">
    <inertial>
      <mass value="10.0"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.0226297" ixy="0" ixz="0" iyy="0.075369" iyz="0" izz="0.0913111"/>
    </inertial>
    <collision name="chassis_collision">
      <origin rpy="0 0 0" xyz="0 0 0.08"/>
      <geometry>
        <box size="1.5062 0.77942 0.324543"/>
      </geometry>
    </collision>
    <visual name="chassis_visual">
      <origin rpy="0 0 0" xyz="0 0 0.08"/>
      <geometry>
        <box size="1.5062 0.77942 0.324543"/>
      </geometry>
    </visual>
  </link>

  <link name="front_left_wheel">
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.145833" ixy="0" ixz="0" iyy="0.145833" iyz="0" izz="0.125"/>
    </inertial>
    <collision name="front_left_wheel_collision">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.25" radius="0.3"/>
      </geometry>
    </collision>
    <visual name="front_left_wheel_visual">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.25" radius="0.3"/>
      </geometry>
    </visual>
  </link>

  <link name="back_left_wheel">
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.145833" ixy="0" ixz="0" iyy="0.145833" iyz="0" izz="0.125"/>
    </inertial>
    <collision name="back_left_wheel_collision">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.25" radius="0.3"/>
      </geometry>
    </collision>
    <visual name="back_left_wheel_visual">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.25" radius="0.3"/>
      </geometry>
    </visual>
  </link>

  <link name="front_right_wheel">
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.145833" ixy="0" ixz="0" iyy="0.145833" iyz="0" izz="0.125"/>
    </inertial>
    <collision name="front_right_wheel_collision">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.25" radius="0.3"/>
      </geometry>
    </collision>
    <visual name="front_right_wheel_visual">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.25" radius="0.3"/>
      </geometry>
    </visual>
  </link>

  <link name="back_right_wheel">
    <inertial>
      <mass value="5"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.145833" ixy="0" ixz="0" iyy="0.145833" iyz="0" izz="0.125"/>
    </inertial>
    <collision name="back_right_wheel_collision">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.25" radius="0.3"/>
      </geometry>
    </collision>
    <visual name="back_right_wheel_visual">
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder length="0.25" radius="0.3"/>
      </geometry>
    </visual>
  </link>

  <joint name="front_right_wheel_joint" type="revolute">
    <parent link="chassis"/>
    <child link="front_right_wheel"/>
    <origin rpy="1.5707 0 0" xyz="0.45309  0.51473 -0.02"/>
    <axis xyz="0 0 1"/>
    <limit effort="-1.0" lower="-1.79769e+308" upper="1.79769e+308" velocity="-1.0"/>
  </joint>

  <joint name="front_left_wheel_joint" type="revolute">
    <parent link="chassis"/>
    <child link="front_left_wheel"/>
    <origin rpy="1.5707 0 0" xyz="0.45309 -0.51473 -0.02"/>
    <axis xyz="0 0 1"/>
    <limit effort="-1.0" lower="-1.79769e+308" upper="1.79769e+308" velocity="-1.0"/>
  </joint>

  <joint name="back_left_wheel_joint" type="revolute">
    <parent link="chassis"/>
    <child link="back_left_wheel"/>
    <origin rpy="1.5707 0 0" xyz="-0.45309 -0.51473 -0.02"/>
    <axis xyz="0 0 1"/>
    <limit effort="-1.0" lower="-1.79769e+308" upper="1.79769e+308" velocity="-1.0"/>
  </joint>

  <joint name="back_right_wheel_joint" type="revolute">
    <parent link="chassis"/>
    <child link="back_right_wheel"/>
    <origin rpy="1.5707 0 0" xyz="-0.45309 0.51473 -0.02"/>
    <axis xyz="0 0 1"/>
    <limit effort="-1.0" lower="-1.79769e+308" upper="1.79769e+308" velocity="-1.0"/>
  </joint>

</robot>
